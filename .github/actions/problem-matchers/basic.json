{
    "problemMatcher": [
        {
            "owner": "pdns-compilation-fatal-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(\\S+):(\\d+):(\\d+): fatal error: (.*)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "pdns-compilation-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(?:(?:^| )\\.\\.|-git\\d+)/([^:]+):(\\d+):(\\d+): error: (.*)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "pdns-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "\\berror:(.*)",
                    "message": 1
                }
            ]
        },
        {
            "owner": "pdns-python-test",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "\bFAILED (test_.*?\\.py)::.* - (\\w+Error: .*)",
                    "file": 1,
                    "message": 2
                }
            ]
        },
        {
            "owner": "pdns-swagger",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(docs/http-api/swagger/.*\\.yaml)$",
                    "file": 1
                },
                {
                    "regexp": "(\\d+):(\\d+)  error  path-params  (.*)",
                    "line": 1,
                    "column": 2,
                    "message": 3
                }
            ]
        },
        {
            "owner": "pdns-sanitizers",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "==ERROR: (\\w+Sanitizer:.*)",
                    "message": 1
                }
            ]
        }
    ]
}
